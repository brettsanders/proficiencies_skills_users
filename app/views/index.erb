<div class="container">
  <h1>User Skills and Levels</h1>

  <%# TODO: display the skills and proficiency levels for each user ... %>
<table border="1">
<tr>
<th>Name</th>
<th>Skill</th>
<th>Years</th>
<th>Formal?</th>
</tr>
<% @proficiencies.each do |proficiency| %>
<tr>
  <td><%= proficiency.user.name %></td>
  <td><%= proficiency.skill.name %></td>
  <td><%= proficiency.years_of_experience %></td>
  <td><%= proficiency.formal_education %></td>
</tr>
<% end %>
</table>
<br>
<a href="/proficiencies/new">Create New Proficiency</a>
